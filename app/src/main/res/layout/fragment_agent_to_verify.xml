<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context="calibrage.payzanagent.fragments.AgentToVerifyFragment"
    android:orientation="vertical">


    <calibrage.payzanagent.controls.CommonTextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="5dp"
        android:gravity="center"
        android:text="Agent Yet To Verify"
        android:textColor="@color/Black"
        android:textSize="25dp" />
    <android.support.v7.widget.RecyclerView
        android:id="@+id/recylerview_card"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/tenDp"
        android:layout_marginRight="@dimen/tenDp"
        android:layout_marginTop="@dimen/fourDp"
        android:nestedScrollingEnabled="false">
    </android.support.v7.widget.RecyclerView>

</LinearLayout>
